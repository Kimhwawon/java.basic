package constructor;

public class Webtoon {
public String name;
public int series;
public String painter;
public boolean complete;

// 생성자 

public Webtoon(String n, int 1, String p, String false serise) 
	name = n;
	series = 1;
	painter = p;
	complete = "false serise";
}

public void getInfo() {
System.out.println("제목 : " + name + ", 회차 : " + series + ", 작가 : " + painter + ", 완결여부 : " + complete);

public void uploadWebtoon() {
	series += +1;
	
}
	
	}}

