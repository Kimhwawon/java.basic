package overriding;

public class Horse extends Animal {

	private int rank;
	
	// 오버라이딩 요건
	// 이름과 리턴자료형 등이
	// 같은 부모쪽 메서드를
	// 중복해서 작성함
	public void howl() {
		System.out.println("히이이잉");
		
	
	}
	public void getHorseInfo() {
		System.out.println("이름 : " + name + ", 나이 : " + age);
		
	}
		
 
	

}
